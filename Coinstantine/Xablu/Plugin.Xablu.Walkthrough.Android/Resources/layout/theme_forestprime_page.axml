<?xml version="1.0" encoding="utf-8"?>
<walker.WalkerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/theme_forestprime_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <Button
            android:id="@+id/theme_forestprime_gettingstarted"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="#ed1a3b"
            android:text="GET STARTED"
            android:gravity="center"
            android:visibility="gone"
            android:layout_alignParentBottom="true" />
        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_above="@id/theme_forestprime_gettingstarted">
            <TextView
                android:id="@+id/theme_forestprime_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textStyle="bold"
                android:paddingTop="40dp"/>
            <android.support.constraint.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/theme_forestprime_guide"
                app:layout_constraintGuide_percent="0.75"/>
            <ImageView
                android:id="@+id/theme_forestprime_image"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:scaleType="centerInside"
                android:adjustViewBounds="true"
                android:paddingTop="40dp"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                app:layout_constraintTop_toBottomOf="@id/theme_forestprime_title"
                app:layout_constraintBottom_toTopOf="@id/theme_forestprime_guide"
                />
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/theme_forestprime_guide"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="70dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="20dp"
                    android:paddingLeft="50dp"
                    android:paddingRight="50dp">
                    <TextView
                        android:id="@+id/theme_forestprime_description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal" />
                </RelativeLayout>
            </ScrollView>
        </android.support.constraint.ConstraintLayout>
    </RelativeLayout>
</walker.WalkerLayout>